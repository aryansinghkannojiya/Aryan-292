#include <stdio.h>

int main() {
    long long n;
    int count = 0;

    printf("Enter an integer: ");
    scanf("%lld", &n);

    // Remove the last digit in each iteration until n becomes 0
    do {
        n /= 10;
        ++count;
    } while (n != 0);

    printf("Number of digits: %d\n", count);
    return 0;
}
