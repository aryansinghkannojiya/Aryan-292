#include <stdio.h>

int main() {
    int num1, num2;
    int sum;

    
    printf("Enter the first two-digit number: ");
    scanf("%d", &num1);

    printf("Enter the second two-digit number: ");
    scanf("%d", &num2);

    
    sum = num1 + num2;

    
    printf("\nSum of %d and %d is: %d\n", num1, num2, sum);

    return 0;
}
