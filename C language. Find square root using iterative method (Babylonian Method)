#include <stdio.h>
#include <math.h>

int main() {
    float num, root, temp;

    printf("Enter a number: ");
    scanf("%f", &num);

    root = num / 2; // Initial guess

    while (fabs(root - temp) > 0.00001) { // Iterate until difference is small
        temp = root;
        root = (num / temp + temp) / 2;
    }

    printf("Square root of %.2f is %.5f\n", num, root);
    return 0;
}
